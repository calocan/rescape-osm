{
  "name": "rescape-osm",
  "version": "0.1.62",
  "description": "OpenStreetMap Helpers for Rescape projects",
  "repository": {
    "type": "git",
    "url": "git@github.com:calocan/rescape-osm.git"
  },
  "author": "calocan <andy@likuski.org>",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "jsnext:main": "esm/index.mjs",
  "files": [
    "lib",
    "esm",
    "src"
  ],
  "scripts": {
    "clear-jest": "jest --clearCache",
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage",
    "build": "rollup -c",
    "prepare": "yarn run install-peers && cross-env ENABLE_INTEGRATION_TESTS=false yarn run clean && yarn run lint && yarn run build",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "yarn run docs:prepare && gitbook build -g reactjs/redux && cp logo/apple-touch-icon.png _book/gitbook/images/apple-touch-icon-precomposed-152.png && cp logo/favicon.ico _book/gitbook/images",
    "docs:watch": "yarn run docs:prepare && gitbook serve",
    "docs:publish": "yarn run docs:clean && yarn run docs:build && cp CNAME _book && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:reactjs/redux gh-pages --force",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn version --patch && yarn publish --non-interactive",
    "publish-minor": "yarn version --minor && yarn publish --non-interactive"
  },
  "dependencies": {
    "@google/maps": "^0.5.5",
    "@turf/bbox": "^6.0.1",
    "@turf/bbox-polygon": "^6.0.1",
    "@turf/center": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "@turf/rhumb-distance": "^6.0.1",
    "@turf/square-grid": "^6.0.2",
    "@turf/turf": "^5.1.6",
    "folktale": "^2.3.2",
    "follow-redirects": "^1.7.0",
    "http-https": "^1.0.0",
    "mapbox-geocoding": "^0.1.5",
    "nominatim-geocoder": "^0.1.4",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "query-overpass": "^1.5.3",
    "ramda": "^0.26.1",
    "request": "^2.88.0",
    "string-similarity": "^3.0.0",
    "xhr": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@types/jest": "^24.0.18",
    "add": "^2.0.6",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babelrc-rollup": "^3.0.0",
    "cross-env": "^5.2.0",
    "dotenv": "8.1.0",
    "eslint": "6.2.0",
    "eslint-loader": "2.2.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "4.2.0",
    "fs-extra": "8.1.0",
    "immutable": "^4.0.0-rc.12",
    "install-peers-cli": "^2.1.1",
    "jest": "^24.9.0",
    "jsdoc-babel": "^0.5.0",
    "longjohn": "^0.2.12",
    "promise": "8.0.3",
    "rescape-helpers": ">=1.1.99",
    "rimraf": "^3.0.0",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "10.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-watch": "4.3.1",
    "yarn": "^1.17.3"
  },
  "peerDependencies": {
    "rescape-helpers": ">=1.1.99",
    "rescape-helpers-test": ">=0.0.28",
    "rescape-log": ">=0.1.13",
    "rescape-ramda": ">=1.6.1",
    "rescape-sample-data": ">=1.2.60"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jestsetup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "sourceMaps": "both",
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "transform-es2015-modules-commonjs-simple",
            {
              "noMangle": true
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  }
}
