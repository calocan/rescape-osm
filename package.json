{
  "name": "rescape-osm",
  "version": "0.3.87",
  "description": "OpenStreetMap Helpers for Rescape projects",
  "repository": {
    "type": "git",
    "url": "git@github.com:calocan/rescape-osm.git"
  },
  "author": "calocan <andy@likuski.org>",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "src/index.mjs",
  "files": [
    "lib",
    "esm",
    "src"
  ],
  "scripts": {
    "clear-jest": "jest --clearCache",
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage",
    "build": "rollup -c",
    "prepare": "yarn run install-peers && cross-env ENABLE_INTEGRATION_TESTS=false yarn run clean && yarn run lint && yarn run build",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "yarn run docs:prepare && gitbook build -g reactjs/redux && cp logo/apple-touch-icon.png _book/gitbook/images/apple-touch-icon-precomposed-152.png && cp logo/favicon.ico _book/gitbook/images",
    "docs:watch": "yarn run docs:prepare && gitbook serve",
    "docs:publish": "yarn run docs:clean && yarn run docs:build && cp CNAME _book && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:reactjs/redux gh-pages --force",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn version --patch && git push && yarn publish --non-interactive",
    "publish-minor": "yarn version --minor && git push && yarn publish --non-interactive"
  },
  "dependencies": {
    "@google/maps": "^1.1.0",
    "@googlemaps/google-maps-services-js": "^3.1.6",
    "@turf/along": "^6.0.1",
    "@turf/area": "^6.0.1",
    "@turf/bbox": "^6.0.1",
    "@turf/bbox-polygon": "^6.0.1",
    "@turf/boolean-within": "^6.0.1",
    "@turf/buffer": "^5.1.5",
    "@turf/center": "^6.0.1",
    "@turf/circle": "^6.0.1",
    "@turf/distance": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "@turf/intersect": "^6.1.3",
    "@turf/length": "^6.0.2",
    "@turf/rhumb-distance": "^6.0.1",
    "@turf/square-grid": "^6.0.2",
    "@turf/turf": "^5.1.6",
    "@turf/union": "^6.0.3",
    "d3-scale": "^3.2.1",
    "d3-scale-chromatic": "^2.0.0",
    "folktale": "^2.3.2",
    "follow-redirects": "^1.10.0",
    "fs": "^0.0.1-security",
    "http-https": "^1.0.0",
    "mapbox-geocoding": "^0.1.5",
    "nominatim-geocoder": "^0.1.4",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "query-overpass": "^1.5.5",
    "request": "^2.88.2",
    "stack-trace": "^0.0.10",
    "string-similarity": "^4.0.1",
    "xhr": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.10.3",
    "@babel/runtime": "^7.8.4",
    "@jest/globals": "^26.6.2",
    "@types/jest": "^26.0.15",
    "add": "^2.0.6",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babelrc-rollup": "^3.0.0",
    "cross-env": "^7.0.0",
    "dotenv": "8.2.0",
    "eslint": "7.13.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "6.2.0",
    "fs-extra": "9.0.1",
    "install-peers-cli": "^2.2.0",
    "jest": "^26.6.3",
    "jsdoc-babel": "^0.5.0",
    "longjohn": "^0.2.12",
    "meow": "^8.0.0",
    "moment": "^2.24.0",
    "promise": "8.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-watch": "4.3.1"
  },
  "peerDependencies": {
    "ramda": "^0.27.0",
    "rescape-helpers": ">=1.1.109",
    "rescape-helpers-test": ">=0.0.38",
    "rescape-log": ">=0.1.13",
    "rescape-ramda": "^1.6.84",
    "rescape-sample-data": ">=1.2.60",
    "rescape-validate": ">=1.3.0"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jestsetup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "jest-environment-node",
    "testURL": "http://localhost",
    "transform": {},
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "transform-es2015-modules-commonjs-simple",
            {
              "noMangle": true
            }
          ],
          [
            "@babel/plugin-transform-runtime",
            {
              "helpers": true,
              "regenerator": true
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
